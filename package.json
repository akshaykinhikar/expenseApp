{
    "name": "expense-app",
    "version": "1.0.0",
    "description": "expense tracking app",
    "main": "server.js",
    "scripts": {
        "start": "node --es-module-specifier-resolution=node backend/dist/server.js",
        "server": "nodemon --es-module-specifier-resolution=node backend/dist/server.js",
        "client": "npm start --prefix expense-app",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "data:import": "node backend/seeder",
        "data:destroy": "node backend/seeder -d",
        "build": "npm install && tsc --build && npm install --prefix expense-app && npm install && npm run build --prefix expense-app",
        "ts-build": "tsc --build --watch",
        "ts-clean": "tsc --build --clean",
        "ts-rebuild": "npm run ts-build && npm run ts-clean"
    },
    "author": "kinhikar.akshay32@gmail.com",
    "license": "MIT",
    "dependencies": {
        "@aws-sdk/client-s3": "^3.529.1",
        "@aws-sdk/s3-request-presigner": "^3.533.0",
        "bcryptjs": "^2.4.3",
        "classnames": "^2.5.1",
        "colors": "^1.4.0",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-async-handler": "^1.1.4",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.10.6",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "typescript": "^5.5.4"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.5",
        "@types/express": "^4.17.21",
        "@types/morgan": "^1.9.9",
        "@types/multer": "^1.4.12",
        "@types/node": "^22.2.0",
        "concurrently": "^5.3.0",
        "nodemon": "^2.0.4",
        "npm-run-all": "^4.1.5",
        "sass": "^1.77.2"
    }
}
